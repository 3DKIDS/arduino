const int buttonPin = 4; //스위치 연결 핀

int buttonState = 0;  // 스위치 상태 변수

void setup() {
  // put your setup code here, to run once:
      Serial.begin(9600);
}

void loop() {
  // put your main code here, to run repeatedly:

        buttonState = digitalRead(buttonPin);
        Serial.println(buttonState);
        delay(500);

}
